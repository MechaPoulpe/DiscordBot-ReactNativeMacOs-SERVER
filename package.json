{
  "name": "BotDiscord-ReactNativeMacOs-TouchBar",
  "version": "0.0.1",
  "description": "Bot Discord who show message in the Touchbar (Macbook pro) when someone mention you",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native-macos/local-cli/cli.js start",
    "postinstall": "find ./node_modules/react-native-macos \\( -name *.h -o -name *.m \\) -print0 | xargs -0 sed -i '' -e 's:<CSSLayout/\\(.*\\)>:\"\\1\":g'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7kfpun/FinanceMacOSReactNative.git"
  },
  "keywords": [
    "react-native-macos",
    "react-native",
    "react",
    "macos",
    "touchbar",
    "Discord",
    "bot",
    "discord.js"
  ],
  "author": "MechaPoulpe <joris.hotton@gmail.com> (https://github.com/MechaPoulpe)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MechaPoulpe/BotDiscord-ReactNativeMacOs-TouchBar/issues"
  },
  "homepage": "https://github.com/MechaPoulpe/BotDiscord-ReactNativeMacOs-TouchBar#readme",
  "dependencies": {
    "discord.js": "^11.1.0",
    "express": "^4.15.3",
    "http": "0.0.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0"
  }
}
